{
	"name": "yowiz",
	"displayName": "yowiz",
	"publisher": "sap",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:sap.loadYowiz"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sap.loadYowiz",
				"title": "Load yowiz",
				"category": "Yowiz"
			}
		]
	},
	"scripts": {
		"vue:prep": "cd ../vue && npm i",
		"vue:cp": "cd ../vue && npm run build && cd ../ext && rm -rf out/media && cp -r  ../vue/dist/ out/media",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"package": "vsce package .",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"run:ws": "node ./out/test/index.js",
		"lint": "eslint ./**/*.ts"
	},
	"dependencies": {
		"strip-ansi": "^5.2.0",
		"ws": "^7.1.2",
		"yeoman-environment": "^2.4.0",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@types/inquirer": "^6.5.0",
		"@types/lodash": "^4.14.145",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/strip-ansi": "^5.2.1",
		"@types/ws": "^6.0.3",
		"@types/yeoman-environment": "^2.3.2",
		"@typescript-eslint/eslint-plugin": "^2.8.0",
		"@typescript-eslint/parser": "^2.8.0",
		"cpx": "^1.5.0",
		"eslint": "^6.6.0",
		"rimraf": "^3.0.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-no-unused-expression-chai": "^0.1.4",
		"typescript": "^3.6.1",
		"vsce": "^1.68.0",
		"vscode": "^1.1.28"
	}
}
