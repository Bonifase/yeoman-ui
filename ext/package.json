{
	"name": "yowiz",
	"displayName": "yowiz",
	"publisher": "sap",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:sap.loadYowiz"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sap.loadYowiz",
				"title": "Load yowiz",
				"category": "Yowiz"
			}
		]
	},
	"scripts": {
		"vue:prep": "cd ../vue && npm i",
		"vue:cp": "cd ../vue && npm run build && cd ../ext && rm -rf out/media && cp -r  ../vue/dist/ out/media",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"package": "vsce package .",
		"test_old": "npm run compile && node ./node_modules/vscode/bin/test",
		"test": "nyc mocha -p tsconfig.json --opts ./mocha.opts",
		"run:ws": "node ./out/test/index.js",
		"lint": "tslint '*/*.{ts,tsx}' -c ./tslint.yml"
	},
	"dependencies": {
		"strip-ansi": "^5.2.0",
		"ws": "^7.1.2",
		"yeoman-environment": "^2.4.0",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.145",
		"@types/inquirer": "^6.5.0",
		"@types/chai": "^4.2.5",
		"@types/sinon": "^7.5.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.12.21",
		"@types/strip-ansi": "^5.2.1",
		"@types/ws": "^6.0.3",
		"@types/yeoman-environment": "^2.3.2",
		"cpx": "^1.5.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-no-unused-expression-chai": "^0.1.4",
		"typescript": "^3.6.1",
		"vsce": "^1.68.0",
		"vscode": "^1.1.28",
		"chai": "^4.2.0",
		"sinon": "^7.5.0",
		"mocha": "^6.2.2",
		"nyc": "^14.1.1",
		"ts-node": "^8.5.2"
	}
}
